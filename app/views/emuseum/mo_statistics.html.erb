      "value": 20
    }, {
      "title": "Subscribers",
      "value": 60
    } ],
    "titleField": "title",
    "valueField": "value",
    "labelRadius": 5,

    "radius": "42%",
    "innerRadius": "60%",
    "labelText": "[[title]]",
    "export": {
      "enabled": true
    }
  } );
  </script>
