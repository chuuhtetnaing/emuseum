		<!-- nav bar +end+ -->
		<%end%>

	<!--Devise Flash Massage +Start+-->
	<!--
		<p class="notice"><%= notice %></p>
    	<p class="alert"><%= alert %></p>
    -->
	<!--Devise Flash Massage +End+-->

    <%= yield %>

    <!-- footer +Start+ -->
		<%= render 'shared/footer' %>	
